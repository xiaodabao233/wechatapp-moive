<view class="container"><view class="search-city"><input class="search-input" placeholder="输入城市名或拼音查询" placeholder-class="phcolor" data-event-opts="{{[['input',[['search',['$event']]]]]}}" bindinput="__e"/><view class="iconfont icon-sousuo"></view></view><block wx:if="{{!!!searchValue}}"><block><view class="citylist-content"><block wx:for="{{citylist}}" wx:for-item="section" wx:for-index="index" wx:key="title"><view class="section" data-title="{{section.title}}"><view class="section-title">{{section.title}}</view><view class="{{[section.style==='inline'?'section-body-inline':'section-body']}}"><block wx:for="{{section.items}}" wx:for-item="city" wx:for-index="index1" wx:key="id"><view class="section-item" data-city="{{city}}" data-event-opts="{{[['tap',[['selectCity',['$event']]]]]}}" bindtap="__e">{{city.city_name}}</view></block></view></view></block></view><view class="citylist-nav" id="test" data-event-opts="{{[['touchmove',[['handleTouchmove',['$event']]]],['touchstart',[['handleTouchstart',['$event']]]],['touchend',[['handleTouchend',['$event']]]]]}}" catchtouchmove="__e" bindtouchstart="__e" bindtouchend="__e"><block wx:for="{{citylist}}" wx:for-item="item" wx:for-index="index" wx:key="title"><view class="citylist-nav-item" data-index="{{index}}" data-event-opts="{{[['tap',[['navSelect',['$event']]]]]}}" bindtap="__e">{{item.style==='inline'?item.index:item.title}}</view></block></view></block></block><block wx:else><block><view class="result-box"><block wx:if="{{$root.g0}}"><block><block wx:for="{{result}}" wx:for-item="city" wx:for-index="index" wx:key="id"><view class="result-item" data-city="{{city}}" data-event-opts="{{[['tap',[['selectCity',['$event']]]]]}}" bindtap="__e">{{''+city.city_name+''}}</view></block></block></block><block wx:else><block>"}"></block></block></view></block></block></view>