<block wx:if="{{cinemaDetail}}"><view><view><cinema-map vue-id="50e2a4e6-1" compName="cinemaMap" data="{{$root.a0}}" bind:__l="__l"></cinema-map></view><view><view><select-movie vue-id="50e2a4e6-2" movies="{{movies}}" defaultSelectID="{{movieId}}" data-event-opts="{{[['^selectMovie',[['selectMovie']]]]}}" bind:selectMovie="__e" bind:__l="__l"></select-movie></view><block wx:if="{{movie}}"><view class="movie-info"><view class="movie-title line-ellipsis"><text class="title">{{movie.nm}}</text><text class="grade"><block wx:if="{{!movie.globalReleased}}"><text>{{''+movie.wish+''}}<text class="small">人想看</text></text></block><block wx:else><block wx:if="{{movie.sc!=='0.0'}}"><text>{{''+movie.sc+''}}<text class="small">分</text></text></block><block wx:else><text><text class="small">暂无评分</text></text></block></block></text></view><view class="movie-desc line-ellipsis">{{movie.desc}}</view></view></block></view><view><select-time vue-id="50e2a4e6-3" days="{{days}}" defaultSelect="{{day}}" data-event-opts="{{[['^selectDayEvent',[['selectDay']]]]}}" bind:selectDayEvent="__e" bind:__l="__l"></select-time></view><view><block wx:if="{{$root.g0}}"><view><block wx:for="{{timeList}}" wx:for-item="item" wx:for-index="index" wx:key="tm"><view class="time item" data-info="{{item}}" data-event-opts="{{[['tap',[['buyTicket',['$event']]]]]}}" bindtap="__e"><view class="time-block box"><view class="begin">{{item.tm}}</view><view class="end">{{item.endTime+" 散场"}}</view></view><view class="info-block box"><view class="lan line-ellipsis">{{item.lang+" "+item.tp}}</view><view class="hall line-ellipsis">{{item.th}}</view></view><view class="price box"><view class="price-box line-ellipsis"><view class="sellPr"><text class="stonefont">{{item.vipPrice&&item.vipPrice*1+10||37}}</text><text class="d">元</text></view><block wx:if="{{item.vipPriceName||item.vipPriceNameNew}}"><view class="vipPrice"><text class="icon">{{item.vipPriceName||item.vipPriceNameNew}}</text><text class="num">{{item.vipPrice}}</text></view></block></view><block wx:if="{{item.extraDesc}}"><view class="discount line-ellipsis">{{item.extraDesc}}</view></block></view><view class="button-block"><view class="button">购票</view></view></view></block></view></block><block wx:else><view class="no-seat"><image src="/static/assets/images/cinema2.png"></image><text>{{movie.globalReleased?'今日场次已映完':'影片未上映'}}</text></view></block></view><block wx:if="{{$root.g1}}"><view class="tuan-list"><view class="title">影院超值套餐</view><block wx:for="{{divideDealList}}" wx:for-item="item" wx:for-index="index" wx:key="title"><view class="tuan-item"><block wx:for="{{item.dealList}}" wx:for-item="i" wx:for-index="index1" wx:key="index1"><view class="snack-item" data-info="{{i}}" data-event-opts="{{[['tap',[['goSnackPage',['$event']]]]]}}" bindtap="__e"><view><image src="{{i.imageUrl}}"></image></view><view class="snack-info"><view class="first-title">{{i.firstTitle}}</view><view class="second-title line-ellipsis">{{i.secondTitle}}</view><view class="snack-price"><view><text class="num">{{i.price}}</text><text class="rmb">元</text><text class="font">{{"影院价:"+i.value+"元"}}</text></view><view class="font">{{i.curNumberDesc}}</view></view></view></view></block></view></block></view></block></view></block>